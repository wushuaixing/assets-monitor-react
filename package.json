    "node-sass": "^4.11.0",
    "optimize-css-assets-webpack-plugin": "^5.0.3",
    "qrcode.react": "^1.0.1",
    "sass-loader": "^7.1.0",
    "sass-resources-loader": "^2.0.1",
    "shelljs": "^0.8.3",
    "style-loader": "^0.23.1",
    "uglify-js": "^3.4.9",
    "uglifyjs-webpack-plugin": "^2.0.1",
    "url-loader": "^1.1.2",
    "webpack": "4.33.0",
    "webpack-bundle-analyzer": "^3.5.2",
    "webpack-cli": "^3.1.0",
    "webpack-dev-server": "^3.1.10"
  }
}
