{
  "name": "reactie",
  "version": "1.0.0",
  "description": "A solution that makes React stack work in IE8",
  "author": "yc",
  "license": "MIT",
  "scripts": {
    "devServer": "node ./server/dev.js",
    "dev": "yarn dev:css && yarn dev:shim && yarn dev:polyfill && yarn dev:dll && yarn dev:index",
    "dev:css": "node ./scripts/css.js",
    "dev:shim": "export NODE_ENV=development&& node ./scripts/shim.js",
    "dev:polyfill": "export NODE_ENV=development&& node ./scripts/polyfill.js",
    "dev:dll": "export NODE_ENV=development&& webpack --config ./scripts/dll.js",
    "dev:index": "export NODE_ENV=development&& webpack --config ./scripts/index.js",
    "start": "export NODE_ENV=development&& webpack-dev-server -d --host localhost --config ./scripts/index.js",
    "start:w": "set NODE_ENV=development&& webpack-dev-server -d  --host localhost --config ./scripts/index.js",
    "build": "yarn build:css && yarn build:shim && yarn build:polyfill && yarn build:dll && yarn build:index",
    "build:css": "node ./scripts/css.js",
    "build:shim": "node ./scripts/shim.js",
    "build:polyfill": "node ./scripts/polyfill.js",
    "build:dll": "webpack --config ./scripts/dll.js",
    "build:index": "webpack --config ./scripts/index.js",
    "lint": "eslint ./src --ext .js --ext .jsx",
    "lintfix": "eslint --fix ./src"
  },
  "dependencies": {
    "anujs": "1.5.0",
    "axios": "0.18.0",
    "console-polyfill": "0.3.0",
    "es5-shim": "4.5.11",
    "es6-proxy-polyfill": "1.2.1",
    "jsencrypt": "3.0.0-rc.1",
    "lodash": "4.17.15",
    "matchmedia-polyfill": "0.3.1",
    "react-loadable": "^5.5.0",
    "react-redux": "5.0.7",
    "universal-cookie": "4.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.1.0",
    "@babel/plugin-proposal-class-properties": "^7.1.0",
    "@babel/plugin-syntax-dynamic-import": "^7.0.0",
    "@babel/polyfill": "^7.0.0",
    "@babel/preset-env": "^7.1.0",
    "@babel/preset-react": "^7.0.0",
    "autoprefixer": "^9.2.0",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^8.0.2",
    "copy-webpack-plugin": "^4.5.2",
    "css-loader": "^1.0.0",
    "cssnano": "^4.1.4",
    "del": "^5.0.0",
    "eslint": "^5.8.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-import-resolver-webpack": "^0.11.1",
    "eslint-loader": "^2.1.2",
    "eslint-plugin-import": "^2.17.1",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-react": "^7.12.4",
    "eslint-plugin-react-hooks": "^1.6.1",
    "file-loader": "^3.0.1",
    "html-webpack-plugin": "^3.2.0",
    "http-proxy-middleware": "^0.19.1",
    "koa": "^2.5.1",
    "koa-bodyparser": "^4.2.1",
    "koa-cors": "^0.0.16",
    "koa-static": "^4.0.3",
    "lodash-es": "^4.17.11",
    "mini-css-extract-plugin": "^0.8.0",
    "node-sass": "^4.11.0",
    "optimize-css-assets-webpack-plugin": "^5.0.3",
    "sass-loader": "^7.1.0",
    "sass-resources-loader": "^2.0.1",
    "shelljs": "^0.8.3",
    "style-loader": "^0.23.1",
    "uglify-js": "^3.4.9",
    "uglifyjs-webpack-plugin": "^2.0.1",
    "url-loader": "^1.1.2",
    "webpack": "^4.19.1",
    "webpack-bundle-analyzer": "^3.5.2",
    "webpack-cli": "^3.1.0",
    "webpack-dev-server": "^3.1.10"
  }
}
